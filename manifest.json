{
  "manifest_version": 2,
  "name": "Block Shortcuts",
  "description": "Disallow to register keyboard shortcuts",
  "version": "0.1",
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "run_at": "document_start",
      "js": ["index.js"],
      "all_frames": true
    }
  ]
}
